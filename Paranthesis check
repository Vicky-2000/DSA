#include<stdio.h>
#include<string.h>
int stack[20];
void push(char)
char pop()
int top=-1;
int f=0;
int main()
{char a[20];
    printf("Enter the string:-\n");
    gets(a);
    for(int i=0;i<strlen(a);i++)
    {
        if(a[i]=='('||a[i]=='['||a[i]=='{')
        {
            push(a[i]);
         }
         else{
             if(a[i]==')'||a[i]==']'||a[i]=='}')
             {
                 if(top==-1)
                 {
                     f=1;
                 }
                 else{char t=pop();
                     if(a[i]==')'&&(t=='['||t=='{'))
f=1;
if(a[i]=='}'&&(t=='('||t=='['))
f=1;
if(a[i]==']'&&(t=='{'||t=='('))
f=1;
                 }
             }
         }
    }
    if(top>=0)
    {
        f==1;
    }
    if(f==1)
printf("Unbalanced\n");
else
printf("Balanced\n");
return 0;
}
void push(char a)
{
stack[++top]=a;
}
char pop()
{
return stack[top--];
}
